library(readr)
library(ggplot2)

# URL del archivo en GitHub
url <- "https://raw.githubusercontent.com/TU_USUARIO/TU_REPOSITORIO/main/Incites_CONICET_2019-2023.csv"

# Cargar los datos desde el archivo en GitHub
data <- read_csv(url)

# Graficar la cantidad de documentos por a침o
ggplot(data, aes(x = Publication.Year, y = `Web.of.Science.Documents`)) +
  geom_line() +
  geom_point() +
  labs(x = "A침o de Publicaci칩n", y = "Cantidad de Documentos",
       title = "Cantidad de Documentos de Web of Science por A침o")
